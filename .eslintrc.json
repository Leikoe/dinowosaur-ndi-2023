{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "next/core-web-vitals",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": "off",
    "react/prop-types": "off",
    "indent": ["error", 2]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}

